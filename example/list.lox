var l = [1, 2, 3, "hello", "world"];

print "List l = "; println l;
println "------";

print "length: ";
println length(l);
println "------";

print "removing 'hello' at index 3: ";
delete(l, 3);
println l;
println "------";

print "adding 2 new items (10 and 'Navid'): ";
append(l, 10);
append(l, "Navid");
println l;
println "------";

print "length: ";
println length(l);
println "------";

println "using length in for loop:";
for (var i = 0; i < length(l); i = i + 1) println l[i];
